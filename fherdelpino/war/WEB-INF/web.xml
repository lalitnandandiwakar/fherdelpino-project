<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>Fherdelpino</servlet-name>
		<servlet-class>com.example.fherdelpino.FherdelpinoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>HelloServers</servlet-name>
		<servlet-class>com.example.fherdelpino.HelloServers</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>guestbook</servlet-name>
		<servlet-class>com.example.guestbook.GuestbookServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>sign</servlet-name>
		<servlet-class>com.example.guestbook.SignGuestbookServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DataStoreInsert</servlet-name>
		<servlet-class>com.example.datastore.InsertServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Fherdelpino</servlet-name>
		<url-pattern>/fherdelpino</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HelloServers</servlet-name>
		<url-pattern>/helloserver</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>guestbook</servlet-name>
		<url-pattern>/guestbook</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>sign</servlet-name>
		<url-pattern>/sign</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DataStoreInsert</servlet-name>
		<url-pattern>/DataStoreInsert</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
